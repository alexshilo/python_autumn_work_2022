<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="038fc180d1ee11e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_MaterialThermal</Name>
  <Definition>A set of thermal material properties.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcMaterial</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcMaterial</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="07234880d1ee11e1800000215ad4efdf">
      <Name>SpecificHeatCapacity</Name>
      <Definition>Defines the specific heat of the material: heat energy absorbed per temperature unit.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcSpecificHeatCapacityMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Specific Heat Capacity</NameAlias>
        <NameAlias lang="ru-RU">Удельная теплоемкость</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Определяет удельную теплоемкость материала, то есть, количество поглощаемой тепловой энергии на единицу температуры.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="0be7fc80d1ee11e1800000215ad4efdf">
      <Name>BoilingPoint</Name>
      <Definition>The boiling point of the material (fluid).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcThermodynamicTemperatureMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Boiling Point</NameAlias>
        <NameAlias lang="ru-RU">Точка кипения </NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Точка кипения материала (жидкости).</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="11dddd80d1ee11e1800000215ad4efdf">
      <Name>FreezingPoint</Name>
      <Definition>The freezing point of the material (fluid).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcThermodynamicTemperatureMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Freezing Point</NameAlias>
        <NameAlias lang="ru-RU">Точка замерзания</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Точка замерзания материала (жидкости).</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="17d3be80d1ee11e1800000215ad4efdf">
      <Name>ThermalConductivity</Name>
      <Definition>The rate at which thermal energy is transmitted through the material.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcThermalConductivityMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Thermal Conductivity</NameAlias>
        <NameAlias lang="ru-RU">Теплопроводность</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Скорость, с которой тепловая энергия передается через материал.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAliases lang="ru-RU">Набор тепловых свойств материала.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>