<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="2f5a7980d1e511e1800000215ad4efdf" templatetype="PSET_PERFORMANCEDRIVEN" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_FilterPHistory</Name>
  <Definition>Filter performance history attributes.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcFilter</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcFilter</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="35505a80d1e511e1800000215ad4efdf">
      <Name>CountedEfficiency</Name>
      <Definition>Filter efficiency based the particle counts concentration before and after filter against particles with certain size distribution.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries" />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Counted Efficiency</NameAlias>
        <NameAlias lang="ru-RU">Подсчитанная эффективность</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Эффективность фильтра основывается на концентрации частиц до и после фильтрации против частиц с определенным распределением по размерам.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="397c7800d1e511e1800000215ad4efdf">
      <Name>WeightedEfficiency</Name>
      <Definition>Filter efficiency based the particle weight concentration before and after filter against particles with certain size distribution.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries" />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Weighted Efficiency</NameAlias>
        <NameAlias lang="ru-RU">Взвешенная эффективность</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Эффективность фильтра основывается на концентрации массы частиц до и после фильтрации против частиц с определенным распределением по размерам.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="3c776880d1e511e1800000215ad4efdf">
      <Name>ParticleMassHolding</Name>
      <Definition>Mass of particle holding in the filter.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcTimeSeries" />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Particle Mass Holding</NameAlias>
        <NameAlias lang="ru-RU">Масса содержащейся частицы</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Масса частицы, содержащейся в фильтре.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAliases lang="ru-RU">Атрибуты истории эксплуатации фильтра.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>