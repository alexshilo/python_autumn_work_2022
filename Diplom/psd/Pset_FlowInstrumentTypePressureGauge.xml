<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="b6a8ec80d1e711e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_FlowInstrumentTypePressureGauge</Name>
  <Definition>A device that reads and displays a pressure value at a point or the pressure difference between two points.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcFlowInstrument/PRESSUREGAUGE</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcFlowInstrument/PRESSUREGAUGE</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="bc9ecd80d1e711e1800000215ad4efdf">
      <Name>PressureGaugeType</Name>
      <Definition>Identifies the means by which pressure is displayed.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_PressureGaugeType">
            <EnumItem>DIAL</EnumItem>
            <EnumItem>DIGITAL</EnumItem>
            <EnumItem>MANOMETER</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>DIAL</Name>
              <NameAliases>
                <NameAlias lang="en">Dial</NameAlias>
                <NameAlias lang="ru-RU">Циферблат</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>DIGITAL</Name>
              <NameAliases>
                <NameAlias lang="en">Digital</NameAlias>
                <NameAlias lang="ru-RU">Цифровой</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>MANOMETER</Name>
              <NameAliases>
                <NameAlias lang="en">Manometer</NameAlias>
                <NameAlias lang="ru-RU">Манометр</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Pressure Gauge Type</NameAlias>
        <NameAlias lang="ja-JP">圧力計タイプ</NameAlias>
        <NameAlias lang="ko-KR">압력계 유형</NameAlias>
        <NameAlias lang="ru-RU">Тип манометра</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">圧力が表示される手段を識別する。</DefinitionAlias>
        <DefinitionAlias lang="ko-KR">압력이 표시되는 방법을 확인한다.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Определяет средства, с помощью которых отображается давление.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="caece980d1e711e1800000215ad4efdf">
      <Name>DisplaySize</Name>
      <Definition>The physical size of the display. For a dial pressure gauge it will be the diameter of the dial.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Display Size</NameAlias>
        <NameAlias lang="ja-JP">表示サイズ</NameAlias>
        <NameAlias lang="ko-KR">디스플레이 크기</NameAlias>
        <NameAlias lang="ru-RU">Размер дисплея</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">表示のサイズ。 ダイヤル圧力計に関しては、ダイヤルの直径になる。</DefinitionAlias>
        <DefinitionAlias lang="ko-KR">표시의 크기입니다. 다이얼 게이지에 관해서는 전화 직경된다.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Физический размер дисплея. Диаметр циферблата манометра будет равен диаметру циферблата. </DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAlias lang="ja-JP">その位置の圧力、または、2か所の差圧を測定し表示するデバイス。</PsetDefinitionAlias>
    <PsetDefinitionAliases lang="ru-RU">Устройство, которое считывает и отображает значение давления в точке или разность давления между двумя точками.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>