<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="8281fb80d1e811e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_FlowMeterTypeWaterMeter</Name>
  <Definition>Device that measures, indicates and sometimes records, the volume of water that passes through it without interrupting the flow.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcFlowMeter/WATERMETER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcFlowMeter/WATERMETER</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="87df4600d1e811e1800000215ad4efdf">
      <Name>Type</Name>
      <Definition>Defines the allowed values for selection of the flow meter operation type.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_WaterMeterType">
            <EnumItem>COMPOUND</EnumItem>
            <EnumItem>INFERENTIAL</EnumItem>
            <EnumItem>PISTON</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>COMPOUND</Name>
              <NameAliases>
                <NameAlias lang="en">Compound</NameAlias>
                <NameAlias lang="ru-RU">Расходомер суммарного потока</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>INFERENTIAL</Name>
              <NameAliases>
                <NameAlias lang="en">Inferential</NameAlias>
                <NameAlias lang="ru-RU">Расходомер с косвенным определением параметров потока </NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>PISTON</Name>
              <NameAliases>
                <NameAlias lang="en">Piston</NameAlias>
                <NameAlias lang="ru-RU">Поршневой расходомер</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Type</NameAlias>
        <NameAlias lang="ru-RU">Тип</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Определяет допустимые значения для выбора типа работы счетчика-расходомера.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="93327180d1e811e1800000215ad4efdf">
      <Name>ConnectionSize</Name>
      <Definition>Defines the size of inlet and outlet pipe connections to the meter.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Connection Size</NameAlias>
        <NameAlias lang="ru-RU">Размер соединения</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Определяет размер входных и выходных трубных соединений к счетчику.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="99c0e900d1e811e1800000215ad4efdf">
      <Name>MaximumFlowRate</Name>
      <Definition>Maximum rate of flow which the meter is expected to pass.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcVolumetricFlowRateMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Maximum Flow Rate</NameAlias>
        <NameAlias lang="ru-RU">Максимальная скорость потока</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Предельная скорость потока, которую может пропустить счетчик.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="9fb6ca00d1e811e1800000215ad4efdf">
      <Name>MaximumPressureLoss</Name>
      <Definition>Pressure loss expected across the meter under conditions of maximum flow.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPressureMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Maximum Pressure Loss</NameAlias>
        <NameAlias lang="ru-RU">Максимальная потеря давления</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Потеря давления, ожидаемая на счетчике в условиях предельного потока.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="a47b7e00d1e811e1800000215ad4efdf">
      <Name>BackflowPreventerType</Name>
      <Definition>Identifies the type of backflow preventer installed to prevent the backflow of contaminated or polluted water from an irrigation/reticulation system to a potable water supply.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_BackflowPreventerType">
            <EnumItem>NONE</EnumItem>
            <EnumItem>ATMOSPHERICVACUUMBREAKER</EnumItem>
            <EnumItem>ANTISIPHONVALVE</EnumItem>
            <EnumItem>DOUBLECHECKBACKFLOWPREVENTER</EnumItem>
            <EnumItem>PRESSUREVACUUMBREAKER</EnumItem>
            <EnumItem>REDUCEDPRESSUREBACKFLOWPREVENTER</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>NONE</Name>
              <NameAliases>
                <NameAlias lang="en">None</NameAlias>
                <NameAlias lang="ru-RU">Нет</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>ATMOSPHERICVACUUMBREAKER</Name>
              <NameAliases>
                <NameAlias lang="en">Atmospheric Vacuum Breaker</NameAlias>
                <NameAlias lang="ru-RU">Атмосферный прерыватель вакуума</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>ANTISIPHONVALVE</Name>
              <NameAliases>
                <NameAlias lang="en">Antisiphon Valve</NameAlias>
                <NameAlias lang="ru-RU">Вакуумный вентиль</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>DOUBLECHECKBACKFLOWPREVENTER</Name>
              <NameAliases>
                <NameAlias lang="en">Double Check Backflow Preventer</NameAlias>
                <NameAlias lang="ru-RU">Двойной обратный клапан-детектор</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>PRESSUREVACUUMBREAKER</Name>
              <NameAliases>
                <NameAlias lang="en">Pressure Vacuum Breaker</NameAlias>
                <NameAlias lang="ru-RU">  Вакуумный прерыватель </NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>REDUCEDPRESSUREBACKFLOWPREVENTER</Name>
              <NameAliases>
                <NameAlias lang="en">Reduced Pressure Backflow Preventer</NameAlias>
                <NameAlias lang="ru-RU">Обратный клапан на пониженное давление</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Backflow Preventer Type</NameAlias>
        <NameAlias lang="ru-RU">Тип предохранителя обратного потока</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Определяет тип предохранителя обратного потока, установленный для предотвращения обратного потока зараженной или загрязненной воды из системы ирригации/ретикуляции в питьевую воду.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAliases lang="ru-RU">Устройство, измеряющее, отображающее и периодически записывающее проходящий через него объем воды без изменения потока.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>