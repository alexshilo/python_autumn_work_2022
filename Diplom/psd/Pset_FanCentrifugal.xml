        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_CentrifugalFanDischargePosition">
            <EnumItem>TOPHORIZONTAL</EnumItem>
            <EnumItem>TOPANGULARDOWN</EnumItem>
            <EnumItem>TOPANGULARUP</EnumItem>
            <EnumItem>DOWNBLAST</EnumItem>
            <EnumItem>BOTTOMANGULARDOWN</EnumItem>
            <EnumItem>BOTTOMHORIZONTAL</EnumItem>
            <EnumItem>BOTTOMANGULARUP</EnumItem>
            <EnumItem>UPBLAST</EnumItem>
