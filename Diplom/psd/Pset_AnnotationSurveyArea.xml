<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="5e5ef400d1c111e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_AnnotationSurveyArea</Name>
  <Definition>Specifies particular properties of survey methods to be assigned to survey point set or resulting surface patches</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcAnnotation/SurveyArea</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcAnnotation/SurveyArea</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="63bc3e80d1c111e1800000215ad4efdf">
      <Name>AcquisitionMethod</Name>
      <Definition>The means by which survey data was acquired.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AcquisitionMethod">
            <EnumItem>GPS</EnumItem>
            <EnumItem>LASERSCAN_AIRBORNE</EnumItem>
            <EnumItem>LASERSCAN_GROUND</EnumItem>
            <EnumItem>SONAR</EnumItem>
            <EnumItem>THEODOLITE</EnumItem>
            <EnumItem>USERDEFINED</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>GPS</Name>
              <NameAliases>
                <NameAlias lang="en">GPS</NameAlias>
                <NameAlias lang="ru-RU">GPS</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>LASERSCAN_AIRBORNE</Name>
              <NameAliases>
                <NameAlias lang="en">Laserscan Airborne</NameAlias>
                <NameAlias lang="ru-RU">Система лазерного сканирования воздушного базирования</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>LASERSCAN_GROUND</Name>
              <NameAliases>
                <NameAlias lang="en">Laserscan Ground</NameAlias>
                <NameAlias lang="ru-RU">Система лазерного сканирования наземного базирования</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>SONAR</Name>
              <NameAliases>
                <NameAlias lang="en">Sonar</NameAlias>
                <NameAlias lang="ru-RU">Сонар</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>THEODOLITE</Name>
              <NameAliases>
                <NameAlias lang="en">Theodolite</NameAlias>
                <NameAlias lang="ru-RU">Теодолит</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>USERDEFINED</Name>
              <NameAliases>
                <NameAlias lang="en">Userdefined</NameAlias>
                <NameAlias lang="ru-RU">Определяется пользователем</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">Notknown</NameAlias>
                <NameAlias lang="ru-RU">Неизвестно</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Acquisition Method</NameAlias>
        <NameAlias lang="fr-FR">MethodeAcquisition</NameAlias>
        <NameAlias lang="ru-RU">Способ получения</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="fr-FR">La méthode utilisée pour effectuer le relevé.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Средства, с помощью которых были получены данные съемки.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="76cf0e80d1c111e1800000215ad4efdf">
      <Name>AccuracyQualityObtained</Name>
      <Definition>A measure of the accuracy quality of survey points as obtained expressed in percentage terms.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Accuracy Quality Obtained</NameAlias>
        <NameAlias lang="fr-FR">PrecisionObtenue</NameAlias>
        <NameAlias lang="ru-RU">Полученное качество точности</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="fr-FR">Mesure de la précision obtenue des points de relevé, exprimée en pourcentage.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Измерение полученного качества точности точек съемки, выраженное в процентном представлении.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="7cc4ef80d1c111e1800000215ad4efdf">
      <Name>AccuracyQualityExpected</Name>
      <Definition>A measure of the accuracy quality of survey points as expected expressed in percentage terms.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Accuracy Quality Expected</NameAlias>
        <NameAlias lang="fr-FR">PrecisionAttendue</NameAlias>
        <NameAlias lang="ru-RU">Ожидаемое качество точности</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="fr-FR">Mesure de la précision attendue des points de relevé, exprimée en pourcentage.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Измерение ожидаемого качества точности точек съемки, выраженное в процентном представлении.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAlias lang="fr-FR">Définition de l'IAI : spécifie des propriétés particulières de méthodes de relevé à relier à des ensembles de points de relevé ou aux surfaces résultant de ce relevé.</PsetDefinitionAlias>
    <PsetDefinitionAliases lang="ru-RU">Указывает конкретные свойства способов съемки, которые должны быть назначены набору точек съемки или участкам результирующей поверхности</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>