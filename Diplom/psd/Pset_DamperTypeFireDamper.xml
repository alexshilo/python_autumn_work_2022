<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="b7cf1f00d1d111e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_DamperTypeFireDamper</Name>
  <Definition>Fire damper type attributes.
Pset renamed from Pset_DamperTypeFire to Pset_DamperTypeFireDamper in IFC2x2 Pset Addendum.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcDamper/FIREDAMPER</ClassName>
