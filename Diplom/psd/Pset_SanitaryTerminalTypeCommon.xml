    <PropertyDef ifdguid="93112095298e44d29aea7995d82eb202">
      <Name>Color</Name>
      <Definition>Color selection for this object.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases>
        <DefinitionAlias lang="ru-RU">Ассортимент цветовых решений для данного объекта.</DefinitionAlias>
