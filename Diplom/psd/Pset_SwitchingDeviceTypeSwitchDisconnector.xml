          <EnumList name="PEnum_SwitchDisconnectorType">
            <EnumItem>CENTERBREAK</EnumItem>
            <EnumItem>DIVIDEDSUPPORT</EnumItem>
            <EnumItem>DOUBLEBREAK</EnumItem>
            <EnumItem>EARTHINGSWITCH</EnumItem>
            <EnumItem>ISOLATOR</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
