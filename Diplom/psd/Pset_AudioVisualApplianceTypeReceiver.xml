<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="628c1100d1c311e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_AudioVisualApplianceTypeReceiver</Name>
  <Definition>An audio-visual receiver is a device that switches audio and/or video from multiple sources, including external sources connected from ports and internal aggregated sources.  HISTORY: Added in IFC4.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcAudioVisualAppliance/RECEIVER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcAudioVisualAppliance/RECEIVER</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="6a4bb580d1c311e1800000215ad4efdf">
      <Name>ReceiverType</Name>
      <Definition>Indicates the type of receiver.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AudioVisualReceiverType">
            <EnumItem>AUDIO</EnumItem>
            <EnumItem>AUDIOVIDEO</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET.</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>AUDIO</Name>
              <NameAliases>
                <NameAlias lang="en">Audio</NameAlias>
                <NameAlias lang="ru-RU">Аудио</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>AUDIOVIDEO</Name>
              <NameAliases>
                <NameAlias lang="en">Audio/Video</NameAlias>
                <NameAlias lang="ru-RU">Аудио/видео</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET.</Name>
              <NameAliases>
                <NameAlias lang="en">Unset.</NameAlias>
                <NameAlias lang="ru-RU">Не задано.</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Receiver Type</NameAlias>
        <NameAlias lang="ja-JP">受信機形式</NameAlias>
        <NameAlias lang="ko-KR">수신기 형식</NameAlias>
        <NameAlias lang="ru-RU">Тип приемника</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">受信機の形式を示す。</DefinitionAlias>
        <DefinitionAlias lang="ko-KR">수신기의 형식을 보여준다.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Указывает тип приемника.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="7a639500d1c311e1800000215ad4efdf">
      <Name>AudioAmplification</Name>
      <Definition>Indicates audio amplification frequency ranges.</Definition>
      <PropertyType>
        <TypePropertyTableValue>
          <Expression />
          <DefiningValue>
            <DataType type="IfcFrequencyMeasure" />
          </DefiningValue>
          <DefinedValue>
            <DataType type="IfcRatioMeasure" />
          </DefinedValue>
        </TypePropertyTableValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Audio Amplification</NameAlias>
        <NameAlias lang="ja-JP">オーディオアンプ</NameAlias>
        <NameAlias lang="ko-KR">오디오 앰프</NameAlias>
        <NameAlias lang="ru-RU">Усиление звука</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">再生周波数帯域を示す。</DefinitionAlias>
        <DefinitionAlias lang="ko-KR">재생 주파수 대역을 나타낸다.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Указывает диапазоны частот усиления звука.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="83546680d1c311e1800000215ad4efdf">
      <Name>AudioMode</Name>
      <Definition>Indicates audio sound modes and corresponding labels, if applicable.</Definition>
      <PropertyType>
        <TypePropertyTableValue>
          <Expression />
          <DefiningValue>
            <DataType type="IfcIdentifier" />
          </DefiningValue>
          <DefinedValue>
            <DataType type="IfcLabel" />
          </DefinedValue>
        </TypePropertyTableValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Audio Mode</NameAlias>
        <NameAlias lang="ja-JP">オーディオモード</NameAlias>
        <NameAlias lang="ko-KR">오디오 모드</NameAlias>
        <NameAlias lang="ru-RU">Звуковой режим</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">オーディオサウンドモードの設定。</DefinitionAlias>
        <DefinitionAlias lang="ko-KR">오디오 사운드 모드 설정.</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Указывает звуковые режимы и соответствующие метки, если применимо.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAlias lang="ja-JP">内部や外部に接続された音源を含む複数の情報から、音響と映像信号を切り替える装置。</PsetDefinitionAlias>
    <PsetDefinitionAliases lang="ru-RU">Аудио- видеоприемник – это устройство, которое переключает аудио и/или видео из нескольких источников, включая внешние источники , подсоединенные через разъемы, и внутренние встроенные источники. ИСТОРИЯ: Добавлено в IFC4.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>