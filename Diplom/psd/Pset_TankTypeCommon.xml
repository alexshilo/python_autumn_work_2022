                <NameAlias lang="ru-RU">Вогнуто-плоский</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>FLATFLAT</Name>
              <NameAliases>
                <NameAlias lang="en">Flat Flat</NameAlias>
                <NameAlias lang="ru-RU">Двоякоплоский</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
