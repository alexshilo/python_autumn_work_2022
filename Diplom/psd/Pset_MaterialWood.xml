<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="45206c80d1ee11e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_MaterialWood</Name>
  <Definition>This is a collection of properties applicable to wood-based materials that specify kind and grade of material as well as moisture related parameters.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcMaterial/Wood</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcMaterial/Wood</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="494c8a00d1ee11e1800000215ad4efdf">
      <Name>Species</Name>
      <Definition>Wood species of a solid wood or laminated wood product.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Species</NameAlias>
        <NameAlias lang="ru-RU">Вид материала</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Вид материала на основе цельной или слоистой древесины.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="4ea9d480d1ee11e1800000215ad4efdf">
      <Name>StrengthGrade</Name>
      <Definition>Grade with respect to mechanical strength and stiffness.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Strength Grade</NameAlias>
        <NameAlias lang="ru-RU">Класс прочности</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Класс материала по механической прочности и жесткости.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="54071f00d1ee11e1800000215ad4efdf">
      <Name>AppearanceGrade</Name>
      <Definition>Grade with respect to visual quality.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Appearance Grade</NameAlias>
        <NameAlias lang="ru-RU">Класс по внешнему виду</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Класс материала по внешнему виду.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="59646980d1ee11e1800000215ad4efdf">
      <Name>Layup</Name>
      <Definition>Configuration of the lamination.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Layup</NameAlias>
        <NameAlias lang="ru-RU">Укладка</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Конфигурация слоев древесины.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="5f5a4a80d1ee11e1800000215ad4efdf">
      <Name>Layers</Name>
      <Definition>Number of layers.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcInteger" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Layers</NameAlias>
        <NameAlias lang="ru-RU">Слои</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Количество слоев.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="63866800d1ee11e1800000215ad4efdf">
      <Name>Plies</Name>
      <Definition>Number of plies.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcInteger" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Plies</NameAlias>
        <NameAlias lang="ru-RU">Листы</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Количество листов.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="67b28580d1ee11e1800000215ad4efdf">
      <Name>MoistureContent</Name>
      <Definition>Total weight of moisture relative to oven-dried weight of the wood.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Moisture Content</NameAlias>
        <NameAlias lang="ru-RU">Содержание влаги</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Общий вес влаги относительно веса высушенной в печи древесины.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="6bdea300d1ee11e1800000215ad4efdf">
      <Name>DimensionalChangeCoefficient</Name>
      <Definition>Weighted dimensional change coefficient, relative to 1% change in moisture content.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Dimensional Change Coefficient</NameAlias>
        <NameAlias lang="ru-RU">Коэффициент изменения размеров</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Взвешенный коэффициент изменения размеров при изменении влажности на 1%.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="70a35700d1ee11e1800000215ad4efdf">
      <Name>ThicknessSwelling</Name>
      <Definition>Swelling ratio relative to board depth.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Thickness Swelling</NameAlias>
        <NameAlias lang="ru-RU">Толщина набухания</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Коэффициент набухания относительно глубины доски.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAliases lang="ru-RU">Является набором свойств, применимых к древесным материалам, которые определяют вид и марку материала, а также параметры, связанные с влагой.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>