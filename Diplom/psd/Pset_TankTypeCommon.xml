                <NameAlias lang="ru-RU">Плоско-выпуклый</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>CONVEXCONVEX</Name>
