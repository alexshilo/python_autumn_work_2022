    <PropertyDef ifdguid="bfa1ba00d1f711e1800000215ad4efdf">
      <Name>ScheduledFrequency</Name>
      <Definition>The period of time between expected instantiations of a work order that may have been predefined.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcTimeMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Scheduled Frequency</NameAlias>
        <NameAlias lang="ru-RU">Запланированная периодичность</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Период времени между ожидаемым созданием экземпляра рабочего наряда, который мог быть предварительно определен.</DefinitionAlias>
      </DefinitionAliases>
