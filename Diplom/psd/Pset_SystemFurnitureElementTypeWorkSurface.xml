<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="680f4880d20f11e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_SystemFurnitureElementTypeWorkSurface</Name>
  <Definition>A set of specific properties for work surfaces used in workstations. HISTORY: First issued in IFC Release R1.5. Renamed from Pset_Worksurface</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcSystemFurnitureElement/WORKSURFACE</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcSystemFurnitureElement/WORKSURFACE</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="6cd3fc80d20f11e1800000215ad4efdf">
      <Name>UsePurpose</Name>
      <Definition>The principal purpose for which the work surface is intended to be used e.g. writing/reading, computer, meeting, printer, reference files, etc.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Use Purpose</NameAlias>
        <NameAlias lang="ru-RU">Предназначение</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Основное назначение, по которому предполагается использовать рабочую поверхность, например, письмо/чтение, установка компьютера, принтера, проведение совещаний, хранение справочной информации, и т.д.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="73fb0a80d20f11e1800000215ad4efdf">
      <Name>SupportType</Name>
      <Definition>Available support types from which that required may be selected.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_FurniturePanelType">
            <EnumItem>FREESTANDING</EnumItem>
            <EnumItem>SUPPORTED</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>ACOUSTICAL</Name>
              <NameAliases>
                <NameAlias lang="en">Acoustical</NameAlias>
                <NameAlias lang="ru-RU">Acoustical</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>GLAZED</Name>
              <NameAliases>
                <NameAlias lang="en">Glazed</NameAlias>
                <NameAlias lang="ru-RU">Glazed</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>HORZ_SEG</Name>
              <NameAliases>
                <NameAlias lang="en">Horz Seg</NameAlias>
                <NameAlias lang="ru-RU">Horz Seg</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>MONOLITHIC</Name>
              <NameAliases>
                <NameAlias lang="en">Monolithic</NameAlias>
                <NameAlias lang="ru-RU">Monolithic</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OPEN</Name>
              <NameAliases>
                <NameAlias lang="en">Open</NameAlias>
                <NameAlias lang="ru-RU">Open</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>ENDS</Name>
              <NameAliases>
                <NameAlias lang="en">Ends</NameAlias>
                <NameAlias lang="ru-RU">Ends</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>DOOR</Name>
              <NameAliases>
                <NameAlias lang="en">Door</NameAlias>
                <NameAlias lang="ru-RU">Door</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>SCREEN</Name>
              <NameAliases>
                <NameAlias lang="en">Screen</NameAlias>
                <NameAlias lang="ru-RU">Решетка</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Support Type</NameAlias>
        <NameAlias lang="ru-RU">Тип опоры</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Доступные типы опор, из которых можно выбрать необходимый.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="807f6300d20f11e1800000215ad4efdf">
      <Name>HangingHeight</Name>
      <Definition>The hanging height of the worksurface.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Hanging Height</NameAlias>
        <NameAlias lang="ru-RU">Высота подвеса</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Высота подвеса рабочей поверхности.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="8a08cb00d20f11e1800000215ad4efdf">
      <Name>NominalThickness</Name>
      <Definition>The nominal thickness of the work surface.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Nominal Thickness</NameAlias>
        <NameAlias lang="ru-RU">Номинальная толщина</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Номинальная толщина рабочей поверхности.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="92610600d20f11e1800000215ad4efdf">
      <Name>ShapeDescription</Name>
      <Definition>A description of the shape of the work surface e.g. corner square, rectangle, etc.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Shape Description</NameAlias>
        <NameAlias lang="ru-RU">Описание формы</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ru-RU">Описание формы рабочей поверхности, например, угловая, квадратная, прямоугольная, и т.д.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAliases lang="ru-RU">Набор особых свойств рабочих поверхностей, которые используются в рабочих станциях. ИСТОРИЯ: Впервые выпущено в IFC Release R1.5. Переименовано из Pset_Worksurface.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>