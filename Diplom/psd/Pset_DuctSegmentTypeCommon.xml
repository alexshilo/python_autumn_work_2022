<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="611c1200d1de11e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_DuctSegmentTypeCommon</Name>
  <Definition>Duct segment type common attributes.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcDuctSegment</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcDuctSegment</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="66795c80d1de11e1800000215ad4efdf">
      <Name>Reference</Name>
      <Definition>Reference ID for this specified type in this project (e.g. type 'A-1').</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcIdentifier" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Reference</NameAlias>
        <NameAlias lang="ja-JP">参照記号</NameAlias>
        <NameAlias lang="ru-RU">Ссылка</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">このプロジェクトにおける参照記号(例：A-1)。分類コードではなく内部で使用されるプロジェクトタイプとして使用されるもの。</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Ссылочный идентификатор данного конкретного типа в настоящем проекте  (например, тип «A-1»).</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="6bd6a700d1de11e1800000215ad4efdf">
      <Name>Status</Name>
      <Definition>Status of the element, predominately used in renovation or retrofitting projects. The status can be assigned to as "New" - element designed as new addition, "Existing" - element exists and remains, "Demolish" - element existed but is to be demolished,  "Temporary" - element will exists only temporary (like a temporary support structure).</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_ElementStatus">
            <EnumItem>NEW</EnumItem>
            <EnumItem>EXISTING</EnumItem>
            <EnumItem>DEMOLISH</EnumItem>
            <EnumItem>TEMPORARY</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>NEW</Name>
              <Definition>element designed as new addition</Definition>
              <NameAliases>
                <NameAlias lang="en">New</NameAlias>
                <NameAlias lang="ru-RU">Новый</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
                <DefinitionAlias lang="ru-RU">элемент создан как новая пристройка</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>EXISTING</Name>
              <Definition>element exists and is to remain</Definition>
              <NameAliases>
                <NameAlias lang="en">Existing</NameAlias>
                <NameAlias lang="ru-RU">Существующий</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
                <DefinitionAlias lang="ru-RU">элемент существует и останется без изменений</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>DEMOLISH</Name>
              <Definition>element exists but is to be demolished</Definition>
              <NameAliases>
                <NameAlias lang="en">Demolish</NameAlias>
                <NameAlias lang="ru-RU">Подлежит сносу</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
                <DefinitionAlias lang="ru-RU">элемент существует, но подлежит сносу</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>TEMPORARY</Name>
              <Definition>element will exist only temporarily (such as a temporary support structure)</Definition>
              <NameAliases>
                <NameAlias lang="en">Temporary</NameAlias>
                <NameAlias lang="ru-RU">Временный</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
                <DefinitionAlias lang="ru-RU">элемент будет существовать только временно (например, временная опорная конструкция)</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Status</NameAlias>
        <NameAlias lang="ja-JP">状態</NameAlias>
        <NameAlias lang="ru-RU">Статус</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">要素（主にリノベーションまたは改修プロジェクトにおいて）の状態。 状態は、「新規(New)」－新しく追加される要素。「既存」－要素は存在し、かつ残りもの。「破壊」－要素は存在したが、廃棄されるもの。「一時的」－一時的に存在する要素（一時的にサポートしている構造のようなもの）。</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Статус элемента, который преимущественно используется в проектах по реновации или реконструкции. Возможные значения статуса — "Новый" (элемент считается добавленным), "Существующий" — элемент существует и останется неизменным, "Подлежит сносу" — элемент существует, но должен быть уничтожен,  "Временный" — элемент будет существовать только временно (как временная опорная конструкция).</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="7d1fb380d1de11e1800000215ad4efdf">
      <Name>Shape</Name>
      <Definition>Cross sectional shape. Note that this shape is uniform throughout the length of the segment. For nonuniform shapes, a transition fitting should be used instead.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_DuctSegmentShape">
            <EnumItem>FLATOVAL</EnumItem>
            <EnumItem>RECTANGULAR</EnumItem>
            <EnumItem>ROUND</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>FLATOVAL</Name>
              <NameAliases>
                <NameAlias lang="en">Flatoval</NameAlias>
                <NameAlias lang="ru-RU">Плоско-овальный</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>RECTANGULAR</Name>
              <NameAliases>
                <NameAlias lang="en">Rectangular</NameAlias>
                <NameAlias lang="ru-RU">Прямоугольный</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>ROUND</Name>
              <NameAliases>
                <NameAlias lang="en">Round</NameAlias>
                <NameAlias lang="ru-RU">Круглый</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Shape</NameAlias>
        <NameAlias lang="ja-JP">形状</NameAlias>
        <NameAlias lang="ru-RU">Форма</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">断面形状：
この形状は直管を通じて一定である。変形する形状のためには、かわりに変形継手が使用される。（フラットオーバル、角型、丸型　他）</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Форма поперечного сечения. Обратите внимание, что данная форма является одинаковой по всей длине сегмента. Для неравномерных форм следует использовать переходный фитинг.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="8c066600d1de11e1800000215ad4efdf">
      <Name>WorkingPressure</Name>
      <Definition>Pressure classification as defined by the authority having jurisdiction (e.g., SMACNA, etc.).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPressureMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Working Pressure</NameAlias>
        <NameAlias lang="ja-JP">圧力クラス</NameAlias>
        <NameAlias lang="ru-RU">Рабочее давление</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">管轄権を持つ当局（SMACNAなど）によって定義された圧力分類</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Классификация давления, разработанная органом, имеющим юрисдикцию (например, SMACNA и т. д.).</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="9163b080d1de11e1800000215ad4efdf">
      <Name>PressureRange</Name>
      <Definition>Allowable maximum and minimum working pressure (relative to ambient pressure).</Definition>
      <PropertyType>
        <TypePropertyBoundedValue>
          <DataType type="IfcPressureMeasure" />
        </TypePropertyBoundedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Pressure Range</NameAlias>
        <NameAlias lang="ja-JP">圧力範囲</NameAlias>
        <NameAlias lang="ru-RU">Диапазон давления</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">許容最大・最小動作圧力（周辺圧力との相対値）</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Допустимое максимальное и минимальное рабочее давление (относительно атмосферного давления).</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="9c1e4580d1de11e1800000215ad4efdf">
      <Name>TemperatureRange</Name>
      <Definition>Allowable maximum and minimum temperature.</Definition>
      <PropertyType>
        <TypePropertyBoundedValue>
          <DataType type="IfcThermodynamicTemperatureMeasure" />
        </TypePropertyBoundedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Temperature Range</NameAlias>
        <NameAlias lang="ja-JP">温度範囲</NameAlias>
        <NameAlias lang="ru-RU">Температурный диапазон</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">許容最高・最低温度</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Допустимая максимальная и минимальная температура.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="a3455380d1de11e1800000215ad4efdf">
      <Name>LongitudinalSeam</Name>
      <Definition>The type of seam to be used along the longitudinal axis of the duct segment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcText" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Longitudinal Seam</NameAlias>
        <NameAlias lang="ja-JP">ハゼ</NameAlias>
        <NameAlias lang="ru-RU">Продольный шов</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">ハゼの種類は、ダクト直管の縦方向に使用するハゼの種類</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Тип шва, который будет использоваться вдоль продольной оси канального участка.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="a8a29e00d1de11e1800000215ad4efdf">
      <Name>NominalDiameterOrWidth</Name>
      <Definition>The nominal diameter or width of the duct segment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Nominal Diameter Or Width</NameAlias>
        <NameAlias lang="ja-JP">呼び径・巾</NameAlias>
        <NameAlias lang="ru-RU">Номинальный диаметр или ширина</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">ダクト直管の呼び径またはダクト巾</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Номинальный диаметр или ширина канального участка.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="b0624280d1de11e1800000215ad4efdf">
      <Name>NominalHeight</Name>
      <Definition>The nominal height of the duct segment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Nominal Height</NameAlias>
        <NameAlias lang="ja-JP">呼び高さ</NameAlias>
        <NameAlias lang="ru-RU">Номинальная высота</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">ダクト直管の高さ</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Номинальная высота канального участка.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="b6f0ba00d1de11e1800000215ad4efdf">
      <Name>Reinforcement</Name>
      <Definition>The type of reinforcement, if any, used for the duct segment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Reinforcement</NameAlias>
        <NameAlias lang="ja-JP">補強</NameAlias>
        <NameAlias lang="ru-RU">Арматура</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">ダクト直管に何か使用されている場合の補強種類</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">При наличии, тип арматуры, использующийся для канального участка.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="bce69b00d1de11e1800000215ad4efdf">
      <Name>ReinforcementSpacing</Name>
      <Definition>The spacing between reinforcing elements.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Reinforcement Spacing</NameAlias>
        <NameAlias lang="ja-JP">補強間隔</NameAlias>
        <NameAlias lang="ru-RU">Расстояние между армирующими элементами</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">補強要素間の距離</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Расстояние между армирующими элементами.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAlias lang="ja-JP">ダクト直管タイプ共通属性。</PsetDefinitionAlias>
    <PsetDefinitionAliases lang="ru-RU">Общие атрибуты типов канальных сегментов.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
</PropertySetDef>