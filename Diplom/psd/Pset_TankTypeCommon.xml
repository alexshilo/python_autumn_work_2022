                <NameAlias lang="en">Horizontal Cylinder</NameAlias>
                <NameAlias lang="ru-RU">Горизонтальный цилиндр</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>VERTICALCYLINDER</Name>
