<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="a7d82f00d21311e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4" />
  <Name>Pset_UnitaryEquipmentTypeAirHandler</Name>
  <Definition>Air handler unitary equipment type attributes.
Note that these attributes were formerly Pset_AirHandler prior to IFC2x2.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcUnitaryEquipment/AIRHANDLER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcUnitaryEquipment/AIRHANDLER</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="adce1000d21311e1800000215ad4efdf">
      <Name>AirHandlerConstruction</Name>
      <Definition>Enumeration defining how the air handler might be fabricated.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AirHandlerConstruction">
            <EnumItem>MANUFACTUREDITEM</EnumItem>
            <EnumItem>CONSTRUCTEDONSITE</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>MANUFACTUREDITEM</Name>
              <NameAliases>
                <NameAlias lang="en">Manufactured Item</NameAlias>
                <NameAlias lang="ru-RU">Произведенная единица</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>CONSTRUCTEDONSITE</Name>
              <NameAliases>
                <NameAlias lang="en">Constructed On Site</NameAlias>
                <NameAlias lang="ru-RU">Собранный на площадке</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Air Handler Construction</NameAlias>
        <NameAlias lang="ja-JP">空調機製造</NameAlias>
        <NameAlias lang="ru-RU">Конструкция камеры обработки воздуха</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">空調機の組立方法の定義の列挙</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Перечисление, определяющее способ изготовления камеры обработки воздуха.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="baeaff00d21311e1800000215ad4efdf">
      <Name>AirHandlerFanCoilArrangement</Name>
      <Definition>Enumeration defining the arrangement of the supply air fan and the cooling coil.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AirHandlerFanCoilArrangement">
            <EnumItem>BLOWTHROUGH</EnumItem>
            <EnumItem>DRAWTHROUGH</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>BLOWTHROUGH</Name>
              <NameAliases>
                <NameAlias lang="en">Blow Through</NameAlias>
                <NameAlias lang="ru-RU">Вентиляторная система с элементами оборудования на стороне нагнетания</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>DRAWTHROUGH</Name>
              <NameAliases>
                <NameAlias lang="en">Draw Through</NameAlias>
                <NameAlias lang="ru-RU">Вентиляторная система с элементами оборудования на стороне всасывания</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
                <NameAlias lang="ru-RU">(иное)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение отсутствует в списке.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
                <NameAlias lang="ru-RU">(неизвестно)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение неизвестно.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
                <NameAlias lang="ru-RU">(не задано)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
                <DefinitionAlias lang="ru-RU">Значение не указано.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Air Handler Fan Coil Arrangement</NameAlias>
        <NameAlias lang="ja-JP">空調機ファン・コイル配列</NameAlias>
        <NameAlias lang="ru-RU">Расположение вентилятора камеры обработки воздуха и змеевика</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">給気ファンと冷却コイルの配置の定義列挙</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Перечисление, определяющее расположение вентилятора приточного воздуха и охлаждающего змеевика.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="c6d6c100d21311e1800000215ad4efdf">
      <Name>DualDeck</Name>
      <Definition>Does the AirHandler have a dual deck? TRUE = Yes, FALSE = No.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Dual Deck</NameAlias>
        <NameAlias lang="ja-JP">2層</NameAlias>
        <NameAlias lang="ru-RU">Двойная обшивка</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">空調機は2層になっているか
TRUE=はい、FALSE=Iいいえ</DefinitionAlias>
        <DefinitionAlias lang="ru-RU">Наличие двойной обшивки у AirHandler: (TRUE (ИСТИННО)) или (FALSE (ЛОЖНО)).</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
    <PsetDefinitionAlias lang="ja-JP">エアハンドリングユニットのプロパティ属性設定。</PsetDefinitionAlias>
    <PsetDefinitionAliases lang="ru-RU">Атрибуты типа модульного оборудования камеры обработки воздуха. Обратите внимание, что данные атрибуты до IFC2x2 именовались Pset_AirHandler.</PsetDefinitionAliases>
  </PsetDefinitionAliases>
