              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>RECTANGULAR</Name>
              <NameAliases>
                <NameAlias lang="en">Rectangular</NameAlias>
                <NameAlias lang="ru-RU">Прямоугольный</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
