            </ConstantDef>
            <ConstantDef>
              <Name>LOW</Name>
              <NameAliases>
                <NameAlias lang="en">Low</NameAlias>
                <NameAlias lang="ru-RU">Низкий</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en" />
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
